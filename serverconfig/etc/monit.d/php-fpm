check process php-fpm with pidfile /var/run/php-fpm.pid
    group www
    start program = "/usr/bin/php-cgi --fpm --fpm-config /etc/php/php-fpm.conf"
    stop program = "/bin/bash -c '/usr/bin/test -s /var/run/php-fpm.pid && /bin/kill -TERM `cat /var/run/php-fpm.pid` && /bin/rm -f /var/run/php-fpm.pid'"
    if failed port 9000
        then restart
    if 5 restarts within 5 cycles then timeout

